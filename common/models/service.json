{
  "name": "service",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "initialDate": {
      "type": "date",
      "defaultFn": "now"
    },
    "finalDate": {
      "type": "date"
    },
    "finished": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "client": {
      "type": "belongsTo",
      "model": "client",
      "foreignKey": ""
    },
    "inspectionsAreas": {
      "type": "hasMany",
      "model": "InspectionsArea",
      "foreignKey": ""
    },
    "furnitureInspections": {
      "type": "hasMany",
      "model": "furnitureInspection",
      "foreignKey": ""
    },
    "itemInspections": {
      "type": "hasMany",
      "model": "ItemInspection",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
